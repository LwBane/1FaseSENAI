// Escreva um programa que determine se um número é par e maior que 10.

programa {
  funcao inicio() {
    
  inteiro numero 

  escreva("Digite um número: ")
  leia(numero)

  se(numero % 2 == 0 e numero > 10){
    escreva("O número é par e é maior que 10")
  }
  senao{
    escreva("O número não é par ou não é maior que 10")
  }



  }
}
